<div class="modal custom-modal" id="fork-file-modal">
  <div class="modal-dialog" role="document">
    <form class="modal-content" action="./fork" method="post" enctype="multipart/form-data">
      <div class="modal-header">
        <h5 class="modal-title">Load corpus and fork repo</h5>
        <button type="button" class="close" aria-label="Close" name="close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>

      <div class="modal-body">
        <span title="Select a file to fork">
          <u>Enter URL</u>
          <input name="url" id="fork-from-url" />
          <span><strong>Github</strong> link to corpus <strong>file</strong> to edit</span>
        </span>
      </div>

      <div class="modal-footer">
        <button type="submit" name="submit" class="btn btn-primary" disabled>Load</button>
        <button type="button" name="close" class="btn btn-secondary">Cancel</button>
      </div>
    </form>
  </div>
</div>
