{
  "name": "notatrix",
  "version": "2.3.2",
  "description": "experimental notational system for ud-annotatrix, combines CoNLL-U and CG3",
  "main": "build/js/src",
  "scripts": {
    "test": "mocha -r ts-node/register --max-old-space-size=8192 test/index.js",
    "coverage": "nyc mocha -r ts-node/register --max-old-space-size=8192 test/index.js",
    "document": "jsdoc2md src/*.js > build/docs.md",
    "build": "tsc",
    "build-watch": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keggsmurph21/notatrix.git"
  },
  "author": "Kevin Murphy",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/keggsmurph21/notatrix/issues"
  },
  "homepage": "https://github.com/keggsmurph21/notatrix#readme",
  "dependencies": {
    "minimist": "^1.2.5",
    "underscore": "^1.13.1",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.3",
    "chai": "^4.3.4",
    "istanbul": "^0.4.5",
    "jsdoc-to-markdown": "^7.0.1",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "sinon": "^5.1.1",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  }
}
